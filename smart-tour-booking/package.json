{
  "name": "smart-tour-booking",
  "version": "1.0.0",
  "description": "Your Smart & Reliable Bus Travel Partner",
  "homepage": "https://yogi1106-bot.github.io/smart-tour-booking",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "start": "node backend/server.js",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "deploy": "./deploy.sh",
    "deploy:win": "deploy.bat",
    "deploy:pages": "./deploy-pages.sh",
    "deploy:pages:win": "deploy-pages.bat",
    "build:all": "npm run install-all && npm run build",
    "test": "cd backend && npm test && cd ../frontend && npm test"
  },
  "keywords": [
    "bus",
    "travel",
    "booking",
    "tour"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "gh-pages": "^6.1.1"
  }
}
